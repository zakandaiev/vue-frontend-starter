{
  "name": "vue-frontend-starter",
  "version": "1.3.0",
  "private": false,
  "author": "Oleksandr Zakandaiev",
  "authorUrl": "https://github.com/zakandaiev",
  "description": "Vue FrontEnd Starter is a boilerplate kit for easy building modern web applications with Vue.js",
  "keywords": [
    "vue",
    "vite",
    "builder",
    "frontend",
    "starter",
    "boilerplate",
    "kit",
    "html",
    "css",
    "sass",
    "scss",
    "javascript",
    "js",
    "es6",
    "optimized",
    "autoprefixer",
    "grid",
    "reseter",
    "404"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zakandaiev/vue-frontend-starter"
  },
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "imports": {
    "#root/*": "./*",
    "#core/*": "./core/*",
    "#src/*": "./src/*"
  },
  "browserslist": [
    "last 15 versions",
    "not dead"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "docs": "vite build --base=/vue-frontend-starter --outDir=./docs",
    "lint:js": "eslint ./",
    "lint:js:fix": "eslint ./ --fix",
    "lint:css": "stylelint ./src/**/*.{sass,scss}",
    "lint:css:fix": "stylelint ./src/**/*.{sass,scss} --fix",
    "backend": "nodemon backend"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@vitejs/plugin-vue": "^6.0.4",
    "autoprefixer": "^10.4.24",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.8.0",
    "fastify": "^5.7.4",
    "globals": "^17.3.0",
    "minimist": "^1.2.8",
    "nodemon": "^3.1.14",
    "sass": "^1.97.3",
    "stylelint": "^17.3.0",
    "stylelint-config-clean-order": "^8.0.1",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-scss": "^7.0.0",
    "vite": "^7.3.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-minify": "^2.1.0",
    "vite-plugin-vue-devtools": "^8.0.6"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.7",
    "@tabler/icons-vue": "^3.37.1",
    "pinia": "^3.0.4",
    "reseter.css": "^2.0.0",
    "vue": "^3.5.28",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.3"
  },
  "overrides": {
    "eslint": "^9.39.2"
  }
}
