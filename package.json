  "repository": {
    "type": "git",
    "url": "https://github.com/zakandaiev/vue-frontend-starter"
  },
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "browserslist": [
    "last 15 versions",
    "not dead"
  ],
  "scripts": {
