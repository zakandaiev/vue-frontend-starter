    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "fastify": "^5.5.0",
    "globals": "^16.3.0",
    "minimist": "^1.2.8",
    "nodemon": "^3.1.10",
    "sass": "^1.90.0",
